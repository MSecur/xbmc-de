        <string id="31800">FILME UND TV-SERIEN</string>
        <string id="31801">EINSTELLUNGEN FÜR VIDEOINHALTE FESTLEGEN UND VERWALTUNG DER BIBLIOTHEK. HARDWARE-BESCHLEUNIGUNG (DXVA) AKTIVIEREN.[CR]WIEDERGABE-, DATEILISTEN-, UNTERTITEL-, UND DVD-EINSTELLUNGEN.</string> 

        <string id="31802">MUSIK UND CDS</string>
        <string id="31803">EINSTELLUNGEN FÜR AUDIOINHALTE FESTLEGEN UND VERWALTUNG DER BIBLIOTHEK.[CR]KONFIGURATION VISUELLER EFFEKTE, KARAOKE UND AUDIO-CDS.</string> 

        <string id="31804">FOTOS ORGANISIEREN</string>
        <string id="31805">DIASCHAUS WIEDERGEBEN, FAMILIENFOTOS ANSCHAUEN UND ERINNERUNGEN TEILEN.[CR]BILDER KÖNNEN DIREKT VON DER FESTPLATTE ODER Z.B. EINEM USB-STICK ANGEZEIGT WERDEN.</string> 

        <string id="31806">WETTERVORHERSAGE</string>
        <string id="31807">STÜRMT ODER SCHNEIT ES? SCHEINT DIE SONNE ODER GIEßT ES IN STRÖMEN?[CR]ES KÖNNEN DREI WETTERGEBIETE UND DER WETTERDIENST FESTGELEGT WERDEN.</string> 

        <string id="31808">LOKALES NETZWERK UND INTERNET</string>
        <string id="31809">VERWALTUNG VON UPNP-GERÄTEN, NETZWERK-STREAMS, NAS-SYSTEMEN UND SERVERN.[CR]AKTIVIERUNG DER WEB-BENUTZERSCHNITTSTELLE ZUR VERWALTUNG VON MEDIEN.</string> 

        <string id="31810">AUDIO-, VIDEO- UND HARDWARE-EINSTELLUNGEN</string>
        <string id="31811">AUFLÖSUNG FESTLEGEN, BILDSCHIRM KALIBRIEREN, EXTERNE AUDIO-RECEIVER UND SOUNDKARTE EINSTELLEN.[CR]EINGABEGERÄTE UND ENERGIESPAREINSTELLUNGEN KONFIGURIEREN.</string> 

        <string id="31812">ZUSÄTZLICHE MODULE KONFIGURIEREN</string>
        <string id="31813">BEINAHE UNENDLICHE MÖGLICHKEITEN DURCH ZUSÄTZLICHE MODULE DIE NAHTLOS EINGEBUNDEN WERDEN KÖNNEN.[CR]AUTOMATISIERT INHALTE UND INFORMATIONEN ABRUFEN UND AUTOMATISCHE AKTUALISIERUNGEN FESTLEGEN.</string> 

        <string id="31814">MODERNE DARSTELLUNG IN HIGH DEFINITION</string>
        <string id="31815">SKINS, DESIGNS UND SOUNDS KONFIGURIEREN, SPRACHE UND LÄNDEREINSTELLUNGEN FESTLEGEN.[CR]BILDSCHIRMSCHONER UND GRUNDSÄTZLICHE OPTIONEN KONFIGURIEREN.</string> 

        <string id="31816">EIGENE EINSTELLUNGEN FÜR JEDEN</string>
        <string id="31817">BENUTZERKONTEN KONFIGURIEREN, PASSWÖRTER FESTLEGEN:[CR]GETRENNTE EINSTELLUNGEN, INHALTE UND OPTISCHE DARSTELLUNG FÜR JEDEN BENUTZER.</string> 

        <string id="31818">MENÜS UND INHALTE NACH GESCHMACK</string>
        <string id="31819">EIGENE MENÜPUNKTE, VERKNÜPFUNGEN UND ELEMENTE FESTLEGEN UND KONFIGURIEREN.[CR]OPTISCHE ANPASSUNGEN, WIEDERGABEEINSTELLUNGEN UND HINTERGRÜNDE FESTLEGEN.</string> 
        
        <string id="31820">BIBLIOTHEKEN MIT EIGENEM ERSCHEINUNGSBILD</string>
      	<string id="31821">OPTISCHE DARSTELLUNG FÜR DIE UNTERSCHIEDLICHEN BIBLIOTHEKEN FESTLEGEN.[CR]FILME, TV-SERIEN, KONZERTE, TRAILER, MUSIK ODER VIDEOS - ALLES IST ANPASSBAR.</string>

        <string id="31822">EINSTELLUNGEN FÜR MAXIMALEN KOMFORT</string>
        <string id="31823">SYMBOLE EIN- ODER AUSBLENDEN, KIOSK-MODUS UND FANARTS AKTIVIEREN.[CR]LEISTUNG ANPASSEN, QUALITÄT FESTLEGEN UND EXTRAS KONFIGURIEREN.</string> 
        
	
